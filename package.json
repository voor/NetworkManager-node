{
  "name": "network-manager",
  "version": "0.0.1",
  "description": "NodeJS wrapper for network-manager",
  "main": "index.js",
  "scripts": {
    "lint:fix": "eslint . --fix",
    "lint": "eslint . --cache",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "start": "babel-node index.js",
    "test": "mocha --recursive --require babel-register test/**/**.spec.js"
  },
  "lint-staged": {
    "*.js": [
      "lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voor/network-manager.git"
  },
  "keywords": [
    "wifi",
    "network-manager",
    "networking",
    "linux"
  ],
  "author": "Robert Van Voorhees",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voor/network-manager/issues"
  },
  "homepage": "https://github.com/voor/network-manager#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.64.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "prettier-eslint": "^8.8.1"
  },
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "csvtojson": "^1.1.9"
  }
}
